{
  "require": {
    "craftcms/cms": "^3.4.0",
    "craftcms/redactor": "2.5.0",
    "focuslabllc/craft-cheat-sheet": "2.0.3",
    "nystudio107/craft-seomatic": "3.2.43",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
